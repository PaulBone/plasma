#
# This is a Plasma build file, it will tell Plasma about the programs in
# this directory and what modules they're made from.
#

[import_function]
type = program
modules = [ImportFunction]
c_sources = [import_function.cpp]

[import_from_two_modules]
type = program
modules = [ImportFromTwoModules1, ImportFromTwoModules2]
c_sources = [import_from_two_modules.cpp]

[import_two_sources]
type = program
modules = [ImportTwoSources]
c_sources = [import_two_sources.cpp, extra.cpp]

